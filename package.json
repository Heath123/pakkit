{
  "name": "pakkit",
  "productName": "pakkit",
  "version": "1.0.0",
  "description": "A packet monitor for Minecraft Java and Minecraft Bedrock",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-web": "cp html out/html; rm -r out/html/startPage;",
    "postinstall": "patch-package"
  },
  "author": {
    "name": "Heath Mitchell",
    "email": "heath.mitchell27@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "win32"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^16.0.5",
    "electron-rebuild": "^2.3.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "clusterize.js": "^0.18.1",
    "commander": "^7.1.0",
    "electron-localshortcut": "^3.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "electron-unhandled": "^3.0.2",
    "electron-window-state": "^5.0.3",
    "escape-html": "^1.0.3",
    "md5-file": "^5.0.0",
    "minecraft-data": "^3.36.1",
    "minecraft-folder-path": "^1.1.0",
    "minecraft-protocol": "^1.43.0",
    "node-eval": "^2.0.0",
    "patch-package": "^6.4.7",
    "source-map-support": "^0.5.19",
    "ws": "^7.4.6"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.2",
    "utf-8-validate": "^5.0.3"
  }
}
